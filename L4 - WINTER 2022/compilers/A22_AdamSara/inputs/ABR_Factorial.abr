# Abruzzo Language Example # 

main$ {
    abr_shrt i, n, f;
    abr_chr input;

    f = 1;
    i = 1;
    print("Write a number:");
    in(input);

    while(i<n) {
        f = f * i;
        i = i + 1;
    }

    print("Factorial: " + f);
}
